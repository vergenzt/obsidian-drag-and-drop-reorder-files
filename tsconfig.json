// from https://raw.githubusercontent.com/obsidianmd/obsidian-sample-plugin/refs/heads/master/tsconfig.json
{
  "compilerOptions": {
    "noEmit": true,
    "baseUrl": ".",
    "inlineSourceMap": true,
    "inlineSources": true,
    "target": "ES6",
    "module": "ESNext",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "importHelpers": true,
    "strictNullChecks": true,
    "lib": [
      "DOM",
      "ES5",
      "ES6",
      "ES7"
    ],
  },
  "include": [
    "**/*.ts"
  ]
}
